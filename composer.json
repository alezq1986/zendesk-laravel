{
    "name": "alezq1986/zendesk-laravel",
    "type": "library",
    "description": "Fork of huddledigital. Laravel wrapper for zendesk/zendesk_api_client_php package",
    "keywords": [
        "zendesk"
    ],
    "homepage": "",
    "license": "MIT",
    "authors": [
        {
            "name": "",
            "email": ""
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.4|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "zendesk/zendesk_api_client_php": "2.*|^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Huddle\\Zendesk\\Providers\\ZendeskServiceProvider"
            ],
            "aliases": {
                "Zendesk": "Huddle\\Zendesk\\Facades\\Zendesk"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Huddle\\Zendesk\\": "src"
        }
    },
    "minimum-stability": "stable"
}
